programa {
  funcao inicio() {
    real n1, n2
    real divisao 
    real multiplicacao 
    real soma
    real subtracao 

    escreva("Insira o n1:")
    leia(n1)
    escreva("Insira o n2:")
    leia(n2)

  divisao=n1/n2
  multiplicacao=n1*n2
  soma=n1+n2
  subtracao=n1-n2

escreva("Resultado da divisao: ", divisao)
escreva("\nResultado da multiplicação: ", multiplicacao)
escreva("\nResultado da soma: ", soma)
escreva("\nResultado da subtracao ", subtracao)
    
  }
}
